set titlestring=Firefox
set visualbell
set verbose=9
set maxitems=13

colorscheme sweets_snaka

set focuscontent
set hlsearch
set noscrollbars
set ssli=1
set status=input,location,bookmark,history,tabcount,position

map B :bmarks!<Space>
map e :eijiro<Space>

let g:hintsio="io"
let g:hintchars="HJKLASDFGYUIOPQWERTNMZXCVB"
set hintchars="hjklasdfgyuiopqwertnmzxcvb"

set nextpattern=\bnext\b,\bmore\b,next,^次(の)?ページ,\b次.*,→\b,^>$,^(>>|≫)$,^(>|≫),(>|≫)$
set previouspattern=\bprev|previous\b,prev,^前(の)?ページ,\b前.*,\b←,^<$,^(<<|≪)$,^(<|≪),(<|≪)$ 

" command! -nargs=+ lazy autocmd VimperatorEnter .* <args>
" lazy fmaps -u='mail\.google\.com/mail' c / j k n p o u e x s r a # [ ] ? gi gs gt gd ga gc
" lazy fmaps -u='mail\.google\.com/mail/.*/[0-9a-f]+$' c / j,n k,p n,j p,k o u e x s r a # [ ] ? gi gs gt gd ga gc

